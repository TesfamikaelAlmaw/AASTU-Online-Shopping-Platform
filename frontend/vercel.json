{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	],
	"env": {
		"VITE_NEST_APP_URL": "@vite_nest_app_url"
	}
}